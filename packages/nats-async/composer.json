{
    "name": "nats-php/nats-async",
    "description": "Nats async transport implementation.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "queue",
        "nats"
    ],
    "autoload": {
        "psr-4": {
            "NatsPhp\\Nats\\Async\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NatsPhp\\Nats\\Async\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "v.zanfir",
            "email": "vadimzanfir@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
