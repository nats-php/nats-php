{
    "name": "nats-php/nats",
    "description": "Nats capabilities implementation.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "queue",
        "nats"
    ],
    "autoload": {
        "psr-4": {
            "NatsPhp\\Nats\\": "src/"
        },
        "files": ["src/Internal/Frame/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "NatsPhp\\Nats\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "v.zanfir",
            "email": "vadimzanfir@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "nats-php/nats-transport": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3"
    }
}
